<odoo>
    <data>
        <record model="ir.actions.act_window" id="dpms_programmanager_action_window">
            <field name="name">Program Manager</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">dpms.programmanager</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

         <record model="ir.ui.view" id="dpms_programmanagers_form">
            <field name="name">Program Manager</field>
            <field name="model">dpms.programmanager</field>
            <field name="arch" type="xml">
                <form>
                    <header>

                    </header>
                    <group >
                        <group>
                            <field name="name" />
                            <field name="title" />
                        </group>
                        <group>
                            <field name="department_id" />
                            <field name="user_id" />
                        </group>
                    </group>
                </form>
            </field>
         </record>

        <record model="ir.ui.view" id="dpms_programmanagers_tree">
            <field name="name">Program Managers</field>
            <field name="model">dpms.programmanager</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                    <field name="title" />
                    <field name="department_id" />
                    <field name="user_id" />
                </tree>
            </field>
        </record>

    </data>
</odoo>