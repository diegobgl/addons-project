<openerp noupdate="1">
    <record id="default_password_length_config" model="ir.config_parameter">
        <field name="key">encrypted_vault.encrypted_vault_password_length</field>
        <field name="value">15</field>
    </record>
    <record id="default_use_digits_config" model="ir.config_parameter">
        <field name="key">encrypted_vault.encrypted_vault_use_digits</field>
        <field name="value">True</field>
    </record>
    <record id="default_use_letters_config" model="ir.config_parameter">
        <field name="key">encrypted_vault.encrypted_vault_use_letters</field>
        <field name="value">True</field>
    </record>
    <record id="default_use_special_config" model="ir.config_parameter">
        <field name="key">encrypted_vault.encrypted_vault_use_special</field>
        <field name="value">True</field>
    </record>
</openerp>
