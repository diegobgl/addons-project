<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="seq_project_event" model="ir.sequence">
        <field name="name">Project Event</field>
        <field name="code">project.project</field>
        <field name="padding">6</field>
        <field name="prefix">EV</field>
    </record>

    <record id="seq_project_event_activity" model="ir.sequence">
        <field name="name">Project Event Activity</field>
        <field name="code">project.task.activity</field>
        <field name="padding">6</field>
        <field name="prefix">AC</field>
    </record>

    <record id="seq_project_event_task" model="ir.sequence">
        <field name="name">Project Event Task</field>
        <field name="code">project.task.task</field>
        <field name="padding">6</field>
        <field name="prefix">TA</field>
    </record>
</odoo>
